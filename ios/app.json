{
  "expo": {
    "name": "Cheffrey",
    "slug": "cheffrey",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/app_icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-apple-authentication",
      "expo-secure-store",
      "expo-router"
    ],
    "splash": {
      "image": "./app/assets/logo_stamp.png",
      "resizeMode": "contain",
      "backgroundColor": "#faf1e4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.lkleinbrodt.cheffrey"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lkleinbrodt.cheffrey"
    },
    "web": {
      "favicon": "./app/assets/icon_no_border.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b745f031-af85-4247-8b24-debf753f6c7b"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/b745f031-af85-4247-8b24-debf753f6c7b"
    }
  }
}